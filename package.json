{
  "name": "jobflow-portal",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "install:all": "npm --prefix backend install && npm --prefix frontend install",
    "dev:api": "npm --prefix backend start",
    "dev:app": "npm --prefix frontend run dev",
    "build:app": "npm --prefix frontend run build"
  }
}
